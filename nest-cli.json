{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/practice/src",
  "monorepo": true,
  "root": "apps/practice",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/practice/tsconfig.app.json"
  },
  "projects": {
    "practice": {
      "type": "application",
      "root": "apps/practice",
      "entryFile": "main",
      "sourceRoot": "apps/practice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/practice/tsconfig.app.json"
      }
    },
    "example_02_eclare-module": {
      "type": "application",
      "root": "apps/example_02_eclare-module",
      "entryFile": "main",
      "sourceRoot": "apps/example_02_eclare-module/src",
      "compilerOptions": {
        "tsConfigPath": "apps/example_02_eclare-module/tsconfig.app.json"
      }
    }
  }
}